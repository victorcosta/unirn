<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.unirnapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value=".25" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="TopActivityIndicator" value="gray" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <name>UniRN App</name>
    <description>
		Hello World sample application that responds to the deviceready event.
	</description>
    <author email="atendimento@maxmeio.com" href="http://maxmeio.com">
		Victor Costa
	</author>
    <preference name="phonegap-version" value="3.3.0" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/assets/img/icon/android/icon-36-ldpi.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/assets/img/icon/android/icon-48-mdpi.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/assets/img/icon/android/icon-72-hdpi.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/assets/img/icon/android/icon-96-xhdpi.png" />
    <icon gap:platform="blackberry" src="www/assets/img/icon/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/assets/img/icon/blackberry/icon-80.png" />
    <icon gap:platform="ios" height="57" src="www/assets/img/icon/ios/icon-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/assets/img/icon/ios/icon-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/assets/img/icon/ios/icon-57-2x.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/assets/img/icon/ios/icon-72-2x.png" width="144" />
    <icon gap:platform="webos" src="www/assets/img/icon/webos/icon-64.png" />
    <icon gap:platform="winphone" src="www/assets/img/icon/windows-phone/icon-48.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/assets/img/icon/windows-phone/icon-173.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="www/assets/img/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="www/assets/img/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="www/assets/img/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="www/assets/img/screen/android/screen-xhdpi-portrait.png" />
    <gap:splash gap:platform="blackberry" src="www/assets/img/screen/blackberry/screen-225.png" />
    <gap:splash gap:platform="ios" height="480" src="www/assets/img/screen/ios/screen-iphone-portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/assets/img/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/assets/img/screen/ios/screen-ipad-portrait.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/assets/img/screen/ios/screen-ipad-landscape.png" width="1024" />
    <gap:splash gap:platform="winphone" src="www/assets/img/screen/windows-phone/screen-portrait.jpg" />
    <plugin name="InAppBrowser" value="CDVInAppBrowser" />
    <plugin name="Notification" value="org.apache.cordova.Notification" />
    <gap:plugin name="org.apache.cordova.vibration" version="0.3.7" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <access origin="*" />
</widget>
